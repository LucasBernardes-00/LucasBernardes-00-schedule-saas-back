  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LucasBernardes-00/LucasBernardes-00-schedule-saas-back/issues"
  },
  "homepage": "https://github.com/LucasBernardes-00/LucasBernardes-00-schedule-saas-back#readme",
  "devDependencies": {
    "prisma": "^6.11.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@fastify/awilix": "^8.0.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@prisma/adapter-pg": "^6.11.0",
    "@prisma/client": "^6.11.0",
    "@sinclair/typebox": "^0.34.37",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^24.0.8",
    "awilix": "^12.0.5",
    "bcrypt": "^6.0.0",
    "fastify": "^5.4.0"
  },
  "prisma": {
    "schema": "./prisma"
  }
